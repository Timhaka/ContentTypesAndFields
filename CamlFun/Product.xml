        <pnp:FieldRef ID="{91BF50D9-9045-4CE9-91BE-EBF4AE7DB0DC}" />
        <pnp:FieldRef ID="{F96857CD-9BA5-4C0F-8ABA-6829945989F7}" />
        <pnp:FieldRef ID="{F84FCD0C-B572-48BC-80F8-E593D5C49735}" />
      </pnp:FieldRefs>


    </pnp:ListInstance>
  </pnp:Lists>

</pnp:ProvisioningTemplate>